<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.*?>
<?import javafx.scene.effect.*?>
<?import javafx.embed.swing.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.oth.smplsp.view.RootLayoutController">
   <top>
      <MenuBar BorderPane.alignment="CENTER">
        <menus>
          <Menu mnemonicParsing="false" text="%Menu_File">
            <items>
                  <MenuItem mnemonicParsing="false" onAction="#onActionFileOpen" text="%Menu_File_Open" />
                  <MenuItem mnemonicParsing="false" onAction="#onActionFileSave" text="%Menu_File_Save" />
                  <MenuItem mnemonicParsing="false" onAction="#onActionFileSettings" text="%Menu_File_Settings" />
              <MenuItem mnemonicParsing="false" onAction="#onActionFileExit" text="%Menu_File_Exit" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="%Menu_Edit">
            <items>
                  <MenuItem mnemonicParsing="false" onAction="#onActionEditAdd" text="%Menu_Edit_Add" />
              <MenuItem mnemonicParsing="false" onAction="#onActionEditDelete" text="%Menu_Edit_Delete" />
                  <MenuItem mnemonicParsing="false" onAction="#onActionEditDeleteAll" text="%Menu_Edit_DeleteAll" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="%Menu_Help">
            <items>
              <MenuItem mnemonicParsing="false" onAction="#onActionHelpAbout" text="%Menu_Help_About" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
   </top>
   <bottom>
      <VBox BorderPane.alignment="CENTER">
         <children>
            <Separator prefWidth="200.0" />
            <HBox id="HBox" alignment="CENTER_LEFT" prefHeight="0.0" prefWidth="600.0" spacing="5.0">
               <children>
                  <Label maxHeight="1.7976931348623157E308" maxWidth="-1.0" text="Left status" HBox.hgrow="ALWAYS">
                     <font>
                        <Font size="11.0" fx:id="x3" />
                     </font>
                     <textFill>
                        <Color blue="0.625" green="0.625" red="0.625" fx:id="x4" />
                     </textFill>
                  </Label>
                  <Pane prefHeight="-1.0" prefWidth="-1.0" HBox.hgrow="ALWAYS" />
                  <Label fx:id="lblZoom" font="$x3" maxWidth="-1.0" text="Zoom" textFill="$x4" HBox.hgrow="NEVER" />
                  <Button fx:id="btnZoomMinus" focusTraversable="false" graphicTextGap="0.0" mnemonicParsing="false" onMouseClicked="#handleZoomOut" prefHeight="16.0" prefWidth="16.0" text="-" />
                  <Button fx:id="btnZoomPlus" focusTraversable="false" graphicTextGap="0.0" mnemonicParsing="false" onMouseClicked="#handleZoomIn" prefHeight="16.0" prefWidth="16.0" text="+" />
               </children>
               <padding>
                  <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
               </padding>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </HBox>
         </children>
      </VBox>
   </bottom>
   <center>
      <SplitPane dividerPositions="0.65" orientation="VERTICAL" prefHeight="200.0" prefWidth="160.0" BorderPane.alignment="CENTER">
        <items>
          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="100.0" prefWidth="160.0">
               <children>
                  <TabPane prefHeight="112.0" prefWidth="598.0" tabClosingPolicy="UNAVAILABLE" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                    <tabs>
                      <Tab text="Eingabe">
                           <content>
                              <fx:include fx:id="tab1" source="Tab1.fxml" />
                           </content>
                      </Tab>
                      <Tab text="Lösungsweg Konflikt">
                        <content>
                          <fx:include fx:id="tab2" source="Tab2.fxml" />
                        </content>
                      </Tab>
                        <Tab text="Lösung Konflikt">
                          <content>
                           <fx:include fx:id="tab3" source="Tab3.fxml" />
                          </content>
                        </Tab>
                        <Tab text="Lösungsweg Konfliktfrei">
                          <content>
                            <fx:include fx:id="tab4" source="Tab4.fxml" />
                          </content>
                        </Tab>
                        <Tab text="Lösung Konfliktfrei">
                          <content>
                            <fx:include fx:id="tab5" source="Tab5.fxml" />
                          </content>
                        </Tab>
                    </tabs>
                  </TabPane>
               </children>
            </AnchorPane>
          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="100.0" prefWidth="170.0">
               <children>
                  <HBox prefHeight="161.0" prefWidth="598.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                     <children>
                        <ScrollPane fx:id="scrollpane" onZoom="#handleZoom" onZoomFinished="#handleZoomFinished" prefHeight="100.0" prefWidth="628.0" HBox.hgrow="ALWAYS">
                           <content>
                              <SwingNode fx:id="swingNode">
                              </SwingNode>
                           </content>
                        </ScrollPane>
                        <AnchorPane minWidth="72.0" nodeOrientation="LEFT_TO_RIGHT" HBox.hgrow="SOMETIMES">
                           <children>
                              <Button mnemonicParsing="false" prefHeight="26.0" prefWidth="62.0" text="zurück" AnchorPane.bottomAnchor="5.0" AnchorPane.rightAnchor="5.0" />
                              <Button mnemonicParsing="false" prefHeight="26.0" prefWidth="62.0" text="weiter" AnchorPane.bottomAnchor="35.0" AnchorPane.rightAnchor="5.0" />
                           </children>
                           <HBox.margin>
                              <Insets />
                           </HBox.margin>
                        </AnchorPane>
                     </children>
                  </HBox>
               </children>
            </AnchorPane>
        </items>
      </SplitPane>
   </center>
</BorderPane>
