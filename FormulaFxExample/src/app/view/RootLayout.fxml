<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.embed.swing.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.view.RootLayoutController">
   <center>
      <ScrollPane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <content>
            <SwingNode fx:id="swingNode" />
         </content>
      </ScrollPane>
   </center>
   <bottom>
      <HBox prefHeight="32.0" prefWidth="600.0" BorderPane.alignment="CENTER">
         <children>
            <Label text="Zoom" />
            <Button mnemonicParsing="false" onMouseClicked="#handleZoomOut" text="-" />
            <Button mnemonicParsing="false" onMouseClicked="#handleZoomIn" text="+" />
         </children>
      </HBox>
   </bottom>
</BorderPane>
